

<div class ="wrapper">
  <div id= "dating-background" class="col-lg-10 offset-2 pull-right text-center">
    <br><br>
      <div id="center"><p>
      <%= link_to "All Profiles", profiles_path, class: "btn btn-default btn-lg", id: "all-profiles-button" %>
      </p>
    </div>
      <br><br><br>
          <% @venue_reps.each do |venue_rep| %>
            <div class="col-sm-6 col-md-4">
              <div class="thumbnail">
                <%= image_tag find_parent_profile(venue_rep.profile_id).image.url(:medium) %>
                <div class="caption">
                  <h3><%= link_to find_parent_profile(venue_rep.profile_id).name, profile_path(venue_rep.profile_id) %></h3>
                  <h4><%= find_parent_profile(venue_rep.profile.id).bio %></h4>
                    <%= venue_rep.venue_name %>
                </div>
              </div>
            </div>
          <% end %> 
    <%= will_paginate @profiles %>
  </div>
</div>


