<div id="page-wrapper">  
  <div class="container">
    <h1 class="text-center browse-header">Venue Representative
      <p class= "text-center">
      <%= link_to "All Profiles", profiles_path, class: "btn btn-primary btn-lg", id: "all-profiles-button" %>
      </p>
    </h1>
      <% @venue_reps.each do |venue_rep| %>
        <div class="col-sm-6 col-md-4">
          <div class="thumbnail">
            <%= image_tag find_parent_profile(venue_rep.profile_id).image.url(:medium) %>
            <div class="caption">

              <h3><%= link_to find_parent_profile(venue_rep.profile_id).name, profile_path(venue_rep.profile_id), class: "btn-lg btn-primary" %>
            </div>
          </div>
        </div>
      <% end %> 
  </div>
  <%= will_paginate @venue_reps %> 
</div>
