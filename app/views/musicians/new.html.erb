<h1> NEW Musician </h1>
<%= form_for @musician do |f| %>
	<br/>
	<b> <%= f.label(:instrument)%><br/>
	<%= f.text_field(:instrument)%>
	<br/>
	<%= f.label(:genre)%><br/>
	<%= f.text_field(:genre)%>
	<br/>
	<%= f.fields_for :addresses do |builder| %>
		<p>
		<%= builder.label(:street)%><br/>
		<%= builder.text_field(:street)%>
		<br/>
		<%= builder.label(:city)%> <br/>
		<%= builder.text_field(:city)%>
		<br/>
	  <%= builder.label(:state)%>
		<%= builder.text_field(:state)%>
		<br/>
	  <%= builder.label(:zip)%>
		<%= builder.text_field(:zip)%>
		<br/>
		</p>
  <% end %>
	<%= f.submit("Submit")%>
<%end%>

