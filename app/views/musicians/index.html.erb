<div class="page-wrapper">  
  <div class="container">
    <h1 class="text-center browse-header">Musicians
      <p class= "text-center">
      <%= link_to "All Profiles", profiles_path, class: "btn btn-primary btn-lg", id: "all-profiles-button" %>
      </p>
    </h1>
      <% @musicians.each do |musician| %>
        <div class="col-sm-6 col-md-4">
          <div class="thumbnail">
            <%= image_tag find_parent_profile(musician.profile_id).image.url(:medium) %>
            <div class="caption">

              <h3><%= link_to find_parent_profile(musician.profile_id).name, profile_path(musician.profile_id), class: "btn-lg btn-primary" %>
              <h4>
                <%= musician.instrument %>
                <%= musician.genre %>
              </h4>
            </div>
          </div>
        </div>
      <% end %> 
  </div>
  <%= will_paginate @musicians %> 
</div>
